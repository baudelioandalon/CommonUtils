<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="label" format="string" />

    <declare-styleable name="AKTextField">
        <attr name="label" />
        <attr name="android:inputType" />
        <attr name="android:textColor" />
        <attr name="endIconDrawable" />
        <attr name="endIconMode" />
        <attr name="android:maxLength" />
    </declare-styleable>

    <declare-styleable name="AKAutocomplete">
        <attr name="label" />
        <attr name="android:inputType" />
        <attr name="android:textColor" />
        <attr name="endIconDrawable" />
        <attr name="endIconMode" />
    </declare-styleable>

    <declare-styleable name="AKToolbar">
        <attr name="android:title" />
    </declare-styleable>

    <declare-styleable name="flowBottomNavigation">
        <attr name="mbn_defaultIconColor" format="color" />
        <attr name="mbn_selectedIconColor" format="color" />
        <attr name="mbn_backgroundBottomColor" format="color" />
        <attr name="mbn_circleColor" format="color" />
        <attr name="mbn_countTextColor" format="color" />
        <attr name="mbn_countBackgroundColor" format="color" />
        <attr name="mbn_countTypeface" format="string" />
        <attr name="mbn_rippleColor" format="color" />
        <attr name="mbn_shadowColor" format="color" />
        <attr name="mbn_hasAnimation" format="boolean" />
    </declare-styleable>

</resources>